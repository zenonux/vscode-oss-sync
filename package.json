{
  "name": "vscode-oss-sync",
  "displayName": "OssSync",
  "publisher": "zenonux",
  "description": "oss sync extension for vscode(tencent cos).",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#404040",
    "theme": "dark"
  },
  "activationEvents": [
    "workspaceContains:.vscode/oss-sync.json"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-oss-sync.renameAndUploadFile",
          "when": "!explorerResourceIsFolder && resourcePath =~ /ossSyncAssets/",
          "group": "1_modification"
        },
        {
          "command": "vscode-oss-sync.uploadFile",
          "when": "!explorerResourceIsFolder && resourcePath =~ /ossSyncAssets/",
          "group": "1_modification"
        },
        {
          "command": "vscode-oss-sync.syncFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /ossSyncAssets/",
          "group": "1_modification"
        }
      ],
      "commandPalette": [
        {
          "command": "vscode-oss-sync.renameAndUploadFile",
          "when": "False"
        },
        {
          "command": "vscode-oss-sync.uploadFile",
          "when": "False"
        },
        {
          "command": "vscode-oss-sync.syncFolder",
          "when": "False"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-oss-sync.config",
        "title": "OssSync: Config"
      },
      {
        "command": "vscode-oss-sync.renameAndUploadFile",
        "title": "OssSync: Rename & Upload"
      },
      {
        "command": "vscode-oss-sync.uploadFile",
        "title": "OssSync: Upload"
      },
      {
        "command": "vscode-oss-sync.syncFolder",
        "title": "OssSync: Sync"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": ".vscode/oss-sync.json",
        "url": "./schema/config.schema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "bugs": {
    "url": "https://github.com/zenonux/vscode-oss-sync/issues",
    "email": "zenonux@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenonux/vscode-oss-sync.git"
  },
  "dependencies": {
    "cos-nodejs-sdk-v5": "^2.11.12",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.184",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}